class Solution:
    def minimumCost(self, A: List[int]) -> int:
        α = 51
        β = 51
        for i in range(1, len(A)):
            if A[i] < β:
                β = A[i]
                if β < α:
                    α, β = β, α
            if α == 1 and β == 1:
                break
        return A[0] + α + β
