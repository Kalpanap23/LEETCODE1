class Solution(object):
    def evalRPN(self, tokens):
        """
        :type tokens: List[str]
        :rtype: int
        """
        stack = []
        for i in tokens:
            if i.lstrip('-').isdigit():
                stack.append(int(i))
            else:
                a = stack.pop()
                b = stack.pop()
                if i == '+':
                    stack.append(b + a)
                if i == '-':
                    stack.append(b - a)
                if i == '*':
                    stack.append(b * a)
                if i == '/':
                    stack.append(int(b * 1.0 / a))
        return stack.pop()
